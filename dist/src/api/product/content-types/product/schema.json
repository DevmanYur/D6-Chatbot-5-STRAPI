{
    "kind": "collectionType",
    "collectionName": "products",
    "info": {
        "singularName": "product",
        "pluralName": "products",
        "displayName": "PRODUCT"
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "title": {
            "type": "string"
        },
        "description": {
            "type": "text"
        },
        "picture": {
            "allowedTypes": [
                "images",
                "files",
                "videos",
                "audios"
            ],
            "type": "media",
            "multiple": false
        },
        "price": {
            "type": "decimal"
        },
        "cartitems": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::cartitem.cartitem",
            "mappedBy": "product"
        }
    }
}
